{
  "name": "eslint-plugin-json-schema-descriptions",
  "version": "1.1.0",
  "description": "ESLint Plugin to require descriptions on JSON Schema Properties",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run build && npm run test",
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "eslint ./test --config ./test/eslint.config.json.js",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./recommended": {
      "import": "./dist/recommended/index.js",
      "types": "./dist/recommended/index.d.ts"
    }
  },
  "author": "Fraser Benjamin",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraserbenjamin/eslint-plugin-json-schema-descriptions.git"
  },
  "keywords": [
    "JSON",
    "Schema",
    "ESLint",
    "Plugin"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/eslint": "^8.56.10",
    "eslint": "^9.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.49"
  },
  "dependencies": {
    "eslint-compat-utils": "^0.5.1",
    "jsonc-eslint-parser": "^2.4.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0"
  }
}
